{
  "Jwt": {
    "Key": "your-very-long-secret-key-for-hmac",
    "Issuer": "YourIssuer",
    "Audience": "YourAudience",
    "ExpireMinutes": 60
  },
  "Redis": {
    "Configuration": "redis:6379",
    "InstanceName": "IdentityServer:"
  },
  "SqlServerRetryOptions": {
    "MaxRetryCount": 5,
    "MaxRetryDelay": "00:00:05",
    "ErrorNumbersToAdd": []
  },
  "Cors": {
    "AllowedOrigins": [ "https://yourclientapp.com", "https://anotherclient.com" ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
